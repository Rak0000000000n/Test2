using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class git_test : MonoBehaviour
{
    // типа третья версия
    public int int1; //какая то интовая переменная
    public string str1; //какая то строковая переменная

    // старт
    void Start()
    {
        int var = 0; // создаём новую переменную 
        int1 = var; // обновляем переменную int1
    }

    // упдейт
    void Update()
    {
        int1 = int.Parse(str1); // обновляем переменную int1 через str1
        int1 += 1; // увеличиваем переменную int1
    }
}
